// Fusion defining additional conent areas of HomePage NodeType.

prototype(Capitalclinic.Site:HomePage) < prototype(Capitalclinic.Site:Page) {

    headScripts {
        code = ${q(node).property('code')}
    }


    body {
        content {
            header = Neos.Neos:ContentCollection {
                nodePath = 'header'
            }

            footer = Neos.Neos:ContentCollection {
                nodePath = 'footer'
            }
        }
    }
}
